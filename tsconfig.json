{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist",
    "sourceMap": false,
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": false,
    "downlevelIteration": true,
    "lib": ["ES2020"],
    "types": []
  },
  "include": [
    "plugin.d.ts"
  ],
}